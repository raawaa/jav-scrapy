{
  "name": "jav-scrapy",
  "version": "0.8.4",
  "type": "commonjs",
  "description": "",
  "main": "jav.js",
  "preferGlobal": true,
  "dependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/tunnel": "^0.0.7",
    "axios": "1.9.0",
    "axios-retry": "^4.5.0",
    "chalk": "^5.4.1",
    "cli-progress": "^3.12.0",
    "commander": "12.1.0",
    "puppeteer-core": "^24.28.0",
    "tunnel": "^0.0.6",
    "winreg": "^1.2.5",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/async": "^3.2.25",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^24.10.0",
    "@types/puppeteer-core": "^5.4.0",
    "@types/winreg": "^1.2.36",
    "@yao-pkg/pkg": "^5.8.1",
    "async": "^3.2.6",
    "chai": "^6.2.0",
    "cheerio": "^1.1.2",
    "conventional-changelog-cli": "^4.1.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "husky": "^9.1.7",
    "mocha": "^11.7.4",
    "nexe": "^5.0.0-beta.4",
    "nock": "^14.0.10",
    "nodemon": "^3.1.10",
    "semantic-release": "^24.2.0",
    "temp": "^0.9.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "bin": {
    "jav": "./dist/jav.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "mocha test/*.test.js",
    "prepublish": "npm run build",
    "prepare": "npm run build",
    "build": "tsc",
    "build-binary": "npm run build && npx nexe dist/jav.js --target node20-x64 --output jav-scrapy --build",
    "build-binary:windows": "npm run build && npx nexe dist/jav.js --target node20-win-x64 --output jav-scrapy-windows-x64.exe --build",
    "build-binary:linux": "npm run build && npx nexe dist/jav.js --target node20-linux-x64 --output jav-scrapy-linux-x64 --build",
    "build-binary:macos": "npm run build && npx nexe dist/jav.js --target node20-macos-x64 --output jav-scrapy-macos-x64 --build",
    "build-binary:all": "npm run build && npx nexe dist/jav.js --target node20-win-x64 --output jav-scrapy-windows-x64.exe --build && npx nexe dist/jav.js --target node20-linux-x64 --output jav-scrapy-linux-x64 --build && npx nexe dist/jav.js --target node20-macos-x64 --output jav-scrapy-macos-x64 --build",
    "dev": "ts-node src/jav.ts",
    "dev:watch": "nodemon --exec ts-node src/jav.ts",
    "preinstall": "npm run build",
    "release": "semantic-release",
    "changelog": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s",
    "changelog:first": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s -r 0",
    "version": "node scripts/version.js",
    "version:info": "node scripts/version.js info",
    "version:update": "node scripts/version.js update",
    "version:changelog": "node scripts/version.js changelog",
    "version:tag": "node scripts/version.js tag",
    "version:validate": "node scripts/version.js validate",
    "version:prepare": "node scripts/version.js prepare-release"
  },
  "nexe": {
    "input": "dist/jav.js",
    "output": "jav-scrapy",
    "target": "node20-x64",
    "resources": [
      "dist/**/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raawaa/jav-scrapy.git"
  },
  "keywords": [
    "crawler",
    "scrapy",
    "jav",
    "magnet",
    "torrent",
    "nodejs",
    "typescript"
  ],
  "homepage": "https://github.com/raawaa/jav-scrapy#readme",
  "bugs": {
    "url": "https://github.com/raawaa/jav-scrapy/issues"
  }
}
